def factorize(x):
    factors = []
    count = []
    i = 2
    while x > 1:
        if x % i == 0:
            x = x / i
            if i not in factors:
                factors.append(i)
                count.append(1)
            else:
                count[factors.index(i)]+=1
        else:
            i += 1
    return factors, count


for i in range (50,60):
    factors, count = factorize((i**2)%2623)
    print(str(i)+":")
    print(factors)
    print(count)
    print()
